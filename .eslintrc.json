   
{
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "max-len": [2, 100],
        "max-params": [2, 3],
        "@typescript-eslint/no-unused-vars": 2
    }
}